{
  "name": "pickup-node-api",
  "version": "0.3.1",
  "description": "Node API for PlayPickUp",
  "main": "index.js",
  "repository": "git@github.com:PlayPickUp/pickup-node-api.git",
  "author": "Eric Stout <ericwstout@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon -e ts,json,tsx -r dotenv/config ./src/index.ts",
    "build": "yarn clean && yarn build:express && yarn build:workers",
    "build:express": "node ./scripts/build.js",
    "build:workers": "node ./scripts/build-workers.js",
    "clean": "node ./scripts/clean.js",
    "format": "yarn prettier --write --ignore-unknown .",
    "lint": "eslint .",
    "tsc": "tsc -p ./tsconfig.json",
    "test": "yarn lint && yarn tsc",
    "workers:generateDailyProps": "yarn build:workers && nodemon -r dotenv/config ./dist/workers/generateDailyPropsEmail.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.0",
    "@types/lodash": "^4.14.168",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "helmet": "^4.2.0",
    "juice": "^7.0.0",
    "knex": "^0.21.12",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "parcel-bundler": "^1.12.4",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.22",
    "@types/pg": "^7.14.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/serve-favicon": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "source-map-support": "^0.5.19",
    "ts-loader": "^6.2.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  }
}
